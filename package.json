{
    "name": "nenyoswebsite",
    "version": "0.1.0",
    "private": true,
    "main": "./backend/app.js",
    "scripts": {
        "start": "cd backend && node ./backend/app.js",
        "server": "cd backend && nodemon ./backend/app.js --ignore client ",
        "client": "npm run start --prefix client",
        "all": "npm-run-all --parallel server client",
        "iall": "cd backend && npm i --force && cd client && npm i --force && cd"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "concurrently": "^7.0.0",
        "dotenv": "^16.0.0",
        "eslint-plugin-react": "7.28.0",
        "nodemon": "^2.0.15",
        "npm-run-all": "^4.1.5"
    },
    "dependencies": {
        "all": "^0.0.0",
        "body-parser": "^1.19.2",
        "cors": "^2.8.5",
        "ejs": "^3.1.6",
        "express": "^4.17.3",
        "express-fileupload": "^1.3.1",
        "firebase": "^9.6.8",
        "formidable": "^2.0.1",
        "http-proxy-middleware": "^2.0.4",
        "mongodb": "^4.4.1",
        "mongoose": "^6.2.6",
        "multer": "^1.4.4",
        "oracledb": "^5.3.0",
        "react-uuid": "^1.0.2"
    }
}
