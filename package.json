{
    "name": "nenyoswebsite",
    "version": "0.1.0",
    "private": true,
    "proxy": "http://localhost:8000",
    "scripts": {
        "server": "node server/index.js",
        "client": "cd client && npm run start",
        "build:client": "cd client && npm run build",
        "iall": "npm i && cd client && npm i && cd .. && cd server && npm i",
        "startboth": "npm run client & npm run server"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "eslint-plugin-react": "7.28.0"
    }
}
